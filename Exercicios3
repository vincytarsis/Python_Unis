a = int(input('Entre com valor de A:'))
b = int(input('Entre com valor de B:'))
c = int(input('Entre com valor de C:'))

maior = a
if b > a and b > c:
    maior = b
if c > a and c > b:
    maior = c
menor = a
if b < c and b < a:
    menor = b
if c < b and c < a:
    menor = c

print(f'O menor numero: {menor}')
